{
  "manifest_version": 3,
  "name": "One Tab Only",
  "version": "1.0",
  "description": "Prevents opening duplicate tabs for specific websites.",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Manage One Tab List"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
